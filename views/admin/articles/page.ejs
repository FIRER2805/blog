<body>
    <br>
    <div class="container">
        <% result.articles.rows.forEach(article=>{ %>
            <div class="card">
                <div class="card-header">
                    <h3><%=article.title%></h3>
                </div>
                <div class="card-body">
                    <a class="btn btn-success" href="/<%= article.slug %>">Ver artigo</a>
                </div>
            </div>
            <br>
        <% }); %>
    </div>
</body>
